singularnoun5 = ARTICLE, " ", adjective1 | [ARTICLE, " "] [adjective1, " "] (SQUOTE | SNOUN | NAME | {digit});



pluralnoun0 = pluralnoun1, [[", ", {pluralnoun1, ", "}, " ",], CONJUNCTION, " ", pluralnoun1];

pluralnoun1 = pluralnoun2 | "(", pluralnoun2, ")";

pluralnoun2 = (pluralnoun3 | "(", pluralnoun3, ")"), [" ", (pluralrelativeclause | prepositionalphrase)];

pluralnoun3 = [{digit}, " "], (clause | pluralnoun4), [" ", (pluralrelativeclause | prepositionalphrase)];

pluralnoun4 = ARTICLE, " ", adjective1 | [ARTICLE, " "] [adjective1, " "], (PQUOTE | PNOUN | {digit});



