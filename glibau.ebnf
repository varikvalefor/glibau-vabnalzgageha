pluralverb3 = pluralverb4 | "(", pluralverb4, ")";

pluralverb4 = [adverb, " "], (PVERB | PLINKVERB) [" ", adverb];



plurallinkingverb0 = PLURALLINKINGVERB, [" ", adverb];



singularlinkingverb0 = SINGULARLINKINGVERB, [" ", adverb];



singularnoun1 = singularnoun1 [[", ", {singularnoun1, ", "}, " ",], CONJUNCTION, " ", singularnoun1];

